{
    "name": "SDCC",
    "global": {
        "misc-controls": {
            "type": "list",
            "command": "",
            "group": [
                "c/cpp",
                "asm",
                "linker",
                "elf",
                "s19"
            ],
            "description": ""
        },
        "device": {
            "type": "value",
            "command": "-m",
            "description": "",
            "group": [
                "c/cpp",
                "asm",
                "linker",
                "elf",
                "s19"
            ]
        },
        "specific-options": {
            "type": "list",
            "command": "",
            "group": [
                "c/cpp",
                "asm",
                "linker",
                "elf",
                "s19"
            ]
        },
        "stack-auto": {
            "type": "selectable",
            "command": {
                "true": "--stack-auto",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "use-external-stack": {
            "type": "selectable",
            "command": {
                "true": "--xstack",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "int-long-reent": {
            "type": "selectable",
            "command": {
                "true": "--int-long-reent",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "float-reent": {
            "type": "selectable",
            "command": {
                "true": "--float-reent",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "xram-movc": {
            "type": "selectable",
            "command": {
                "true": "--xram-movc",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "noinit-xram-from-code": {
            "type": "selectable",
            "command": {
                "true": "--no-xinit-opt",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "peep-asm": {
            "type": "selectable",
            "command": {
                "true": "--peep-asm",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "peep-return": {
            "type": "selectable",
            "command": {
                "true": "--peep-return",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "optimize-type": {
            "type": "selectable",
            "command": {
                "speed": "--opt-code-speed",
                "size": "--opt-code-size",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "plain-char-is-signed": {
            "type": "selectable",
            "command": {
                "true": "--fsigned-char",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "out-debug-info": {
            "type": "selectable",
            "command": {
                "true": "--debug",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "allow-unsafe-read": {
            "type": "selectable",
            "command": {
                "true": "--allow-unsafe-read",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        },
        "no-optimize-std-lib": {
            "type": "selectable",
            "command": {
                "true": "--nostdlibcall",
                "false": ""
            },
            "group": [
                "c/cpp",
                "asm"
            ]
        }
    },
    "groups": {
        "c/cpp": {
            "$path": "bin\\sdcc.exe",
            "$includes": {
                "body": "-I${value}"
            },
            "$defines": {
                "body": "-D${key}=${value}"
            },
            "$invoke": {
                "useFile": false,
                "body": "${value}"
            },
            "$output": "-o ${out} ${in}",
            "$outputSuffix": ".rel",
            "$default": [
                "-c"
            ],
            "$language-c": {
                "type": "selectable",
                "command": {
                    "c89": "--std-c89",
                    "sdcc89": "--std-sdcc89",
                    "c95": "--std-c95",
                    "c99": "--std-c99",
                    "sdcc99": "--std-sdcc99",
                    "c11": "--std-c11",
                    "sdcc11": "--std-sdcc11",
                    "c2x": "--std-c2x",
                    "sdcc2x": "--std-sdcc2x",
                    "false": "--std-c99"
                }
            }
        },
        "asm": {
            "$path": "bin\\sdcc.exe",
            "$includes": {
                "body": "-I${value}"
            },
            "$defines": {
                "body": "-D${key}=${value}"
            },
            "$invoke": {
                "useFile": false,
                "body": "${value}"
            },
            "$output": "-o ${out} ${in}",
            "$outputSuffix": ".rel",
            "$default": [
                "-c"
            ]
        },
        "linker": {
            "$path": "bin\\sdcc.exe",
            "$invoke": {
                "useFile": false,
                "body": "${value}"
            },
            "$default": [
                "--out-fmt-ihx"
            ],
            "$matcher": [
                "^\\s+Name",
                "^\\s+[-\\s]+$",
                "^\\s+PAGED EXT. RAM",
                "^\\s+EXTERNAL RAM",
                "^\\s+ROM/EPROM/FLASH"
            ],
            "$mapSuffix": ".mem",
            "$objPathSep": " ",
            "$outputSuffix": ".ihx",
            "$output": "-o ${out} ${in}",
            "$mainFirst": true,
            "$outputBin": {
                "$path": "bin\\packihx.exe",
                "$outputSuffix": ".hex",
                "command": "${linkerOutput}>${output}"
            }
        },
        "s19": {
            "$path": "bin\\sdcc.exe",
            "$invoke": {
                "useFile": false,
                "body": "${value}"
            },
            "$default": [
                "--out-fmt-s19"
            ],
            "$matcher": [
                "^\\s+Name",
                "^\\s+[-\\s]+$",
                "^\\s+PAGED EXT. RAM",
                "^\\s+EXTERNAL RAM",
                "^\\s+ROM/EPROM/FLASH"
            ],
            "$mapSuffix": ".mem",
            "$objPathSep": " ",
            "$outputSuffix": ".s19",
            "$output": "-o ${out} ${in}",
            "$mainFirst": true
        },
        "elf": {
            "$path": "bin\\sdcc.exe",
            "$invoke": {
                "useFile": false,
                "body": "${value}"
            },
            "$default": [
                "--out-fmt-elf"
            ],
            "$matcher": [
                "^\\s+Name",
                "^\\s+[-\\s]+$",
                "^\\s+PAGED EXT. RAM",
                "^\\s+EXTERNAL RAM",
                "^\\s+ROM/EPROM/FLASH"
            ],
            "$mapSuffix": ".mem",
            "$objPathSep": " ",
            "$outputSuffix": ".elf",
            "$output": "-o ${out} ${in}",
            "$mainFirst": true
        }
    }
}